<div class="my-wrapper">
<h1 class="custom-font-1 offset0">My Pics</h1>
<br/>

<% @pics.each do |pic| %>
	<div class="stolen-surfbox span3 offset0">
		<h2 class="custom-font-1">
			Hour <%= pic.hour %>
		</h2>

		<div>
			<%= image_tag pic.image %>
		</div>

		<h3 class="custom-font-2">
			I feel <%= pic.emotion %>.
		</h3>

		<p class="custom-font-3">
			I learned that <%= pic.learned %>.
		</p>
	</div>

<% end %>

</div>